{
  "name": "@ar1s/server",
  "version": "1.0.0",
  "description": "Ar1s platform for ChatGPT.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "ws:up": "pnpm up '@ar1s/*'",
    "build": "tsc --project ./tsconfig.build.json",
    "build:test": "tsc --project ./tsconfig.test.json",
    "stack:up": ". ./test/deps/aavars.sh && zsh ./test/deps/keydb.up.sh && zsh ./test/deps/postgres.up.sh",
    "stack:down": ". ./test/deps/aavars.sh && zsh ./test/deps/keydb.down.sh && zsh ./test/deps/postgres.down.sh",
    "db:push": "pg-migrations apply --directory ./database/migrations",
    "db:pull": "pg-schema --directory ./src/modules/database/schema",
    "test": "./test/entrypoint.sh"
  },
  "ava": {
    "extensions": {
      "js": true,
      "ts": "module"
    }
  },
  "keywords": [],
  "author": "HoJeong Go <seia@outlook.kr>",
  "license": "MIT",
  "devDependencies": {
    "@databases/pg": "^5.4.1",
    "@databases/pg-migrations": "^5.0.2",
    "@databases/pg-schema-cli": "^4.3.1",
    "@databases/pg-typed": "^4.4.1",
    "@types/node": "^18.15.11",
    "@types/tough-cookie": "^4.0.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "ava": "^5.2.0",
    "eslint": "^8.37.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "fastify-inject-with-session": "^1.0.1",
    "tough-cookie": "^4.1.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@ar1s/spec": "link:../spec",
    "@fastify/cookie": "^8.3.0",
    "@fastify/type-provider-typebox": "^3.0.0",
    "@sinclair/typebox": "^0.26.8",
    "argon2": "^0.30.3",
    "bufferutil": "^4.0.7",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "generic-pool": "^3.9.0",
    "nanoid": "^4.0.2",
    "paseto": "^3.1.2",
    "redis": "^4.6.5",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.13.0"
  }
}